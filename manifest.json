{
  "name": "ADD BACKGROUND!",
  "description": "My task is to create canvas element, to set correct background position.",
  "version": "2.9",
  "manifest_version": 2,
  "icons": {
    "128": "icon.png"
  },
  "permissions": [],

  "content_scripts": [
	  {
	  	"matches": ["<all_urls>"],
	  	"js": ["content.js"]
	  }
  ],
  "background": {
    "scripts": [
      "jquery-3.3.1.min.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
